{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote",
        "distribution": "internal",
        "withoutCredentials": true
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "credentialsSource": "remote",
        "withoutCredentials": true
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "production": {
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      }
    }
  }
}